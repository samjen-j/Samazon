@{
}

@model IEnumerable<string> 

<div class="btn-block">
    <a asp-controller="Home" asp-action="Index" asp-route-Category="" asp-route-pageNum="1" class="btn btn-outline-secondary">Home</a>

    @foreach (string c in Model)
    {
        <a asp-controller="Home" asp-action="Index" asp-route-Category="@c" asp-route-pageNum="1" class="btn @(ViewBag.CategorySelect == c ? "btn-primary" : "btn-outline-secondary")">@c </a>
    }
</div>