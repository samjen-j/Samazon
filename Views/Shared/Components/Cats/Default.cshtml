@{
}

@model IEnumerable<string> 

<div class="">
    <a asp-controller="Home" asp-action="Index" asp-route-Category="" asp-route-pageNum="1" class="btn btn-outline-primary" style="width:200px">Home</a><br />

    @foreach (string c in Model)
    {
        <a asp-controller="Home" asp-action="Index" asp-route-Category="@c" asp-route-pageNum="1" style="width:200px" class="btn @(ViewBag.CategorySelect == c ? "btn-dark" : "btn-outline-secondary")">@c </a><br />
    }
</div>